        },
        "destD": {
            "guildID": "[destD-GuildId]",
            "channelIDs": ["[destD-Channel1]"],
            "fromSource": ["source1"]
        },
        "destE": {
            "guildID": "[destE-GuildId]",
            "channelIDs": ["[destE-Channel1]", "[destE-Channel2]", "[destE-Channel3]", "[destE-Channel4]"],
            "fromSource": ["source5", "source6"]
