
<?xml version='1.1' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@2.40">
  <actions>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobAction plugin="pipeline-model-definition@1.7.2"/>
    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction plugin="pipeline-model-definition@1.7.2">
      <jobProperties/>
      <triggers/>
      <parameters/>
      <options/>
    </org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction>
  </actions>
  <description>Wumpus Copy: Linux/Debian amd64&#xd;
&#xd;
A PHP-Based Discord bot that utilizes an individual user&apos;s token to monitor for messages that should be copied to another server that the connected bot belongs to. </description>
  <displayName>WumpusCopy: Linux/Debian (amd64)</displayName>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.plugins.jira.JiraProjectProperty plugin="jira@3.1.1"/>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>-1</daysToKeep>
        <numToKeep>25</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>10</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <com.coravy.hudson.plugins.github.GithubProjectProperty plugin="github@1.31.0">
      <projectUrl>https://github.com/ooobii/wumpus-copy/</projectUrl>
      <displayName></displayName>
    </com.coravy.hudson.plugins.github.GithubProjectProperty>
    <org.jenkinsci.plugins.workflow.job.properties.PipelineTriggersJobProperty>
      <triggers>
        <com.cloudbees.jenkins.GitHubPushTrigger plugin="github@1.31.0">
          <spec></spec>
        </com.cloudbees.jenkins.GitHubPushTrigger>
      </triggers>
    </org.jenkinsci.plugins.workflow.job.properties.PipelineTriggersJobProperty>
  </properties>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2.83">
    <script>/* groovylint-disable-next-line CompileStatic */
pipeline {
    agent {label &apos;linux&apos;}
    stages {
        stage(&apos;Clean Workspace&apos;) {
            steps {
                cleanWs()
            }
        }
        stage(&apos;Fetch Composer &amp; Repository&apos;) {
            parallel {
                stage(&apos;Install Composer&apos;) {
                    steps {
                        sh &apos;&apos;&apos;
                        curl -sS https://getcomposer.org/installer -o composer-setup.php
                        mkdir composer
                        php composer-setup.php --install-dir=./composer --filename=composer
                        rm -rf composer-setup.php
                        &apos;&apos;&apos;
                    }
                }
                stage(&apos;Clone Repository&apos;) {
                    steps {
                        sh &apos;git clone https://github.com/ooobii/wumpus-copy.git&apos;
                    }
                }
            }
        }
        stage(&apos;Install Composer Pkgs&apos;) {
            steps {
                sh &apos;./composer/composer install --no-cache -d ./wumpus-copy&apos;
            }
        }
        stage(&apos;Remove Composer&apos;) {
            steps {
                sh &apos;rm -rf composer&apos;
            }
        }
        stage(&apos;Execute Tests&apos;) {
            steps {
                sh &apos;./wumpus-copy/vendor/phpunit/phpunit/phpunit --configuration ./wumpus-copy/test/phpunit.xml&apos;
            }
        }
        stage(&apos;Build Package&apos;) {
            steps {
                sh &apos;make -C ./wumpus-copy/compile package&apos;
            }
        }
    }

    post {
        always {
            archiveArtifacts artifacts: &apos;wumpus-copy/build/*.deb&apos;, fingerprint: true
            junit &apos;wumpus-copy/test/logs/*.xml&apos;
            sh &apos;git -C wumpus-copy pull&apos;
        }
    }
}
</script>
    <sandbox>true</sandbox>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>